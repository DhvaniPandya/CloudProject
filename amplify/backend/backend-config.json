      "service": "S3",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "S3Trigger965f0f49",
          "attributes": [
            "Name",
            "Arn",
            "LambdaExecutionRole"
          ]
        }
      ]
    }
  },
  "function": {
    "S3Trigger965f0f49": {
      "service": "Lambda",
      "providerPlugin": "awscloudformation",
      "build": true,
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "cloudprojectAPI",
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput",
            "GraphQLAPIKeyOutput"
          ]
        },
        {
          "category": "auth",
          "resourceName": "CloudProject",
          "attributes": [
